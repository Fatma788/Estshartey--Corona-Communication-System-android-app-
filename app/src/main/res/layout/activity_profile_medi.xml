<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile">
    <TextView
        android:id="@+id/blood_txt"
        android:layout_width="258dp"
        android:layout_height="29dp"
        android:layout_marginEnd="188dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/past3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Medical"
        android:layout_width="325dp"
        android:layout_height="72dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"

        android:background="@drawable/image_color"
        android:gravity="center_horizontal|center_vertical"
        android:text="Medical Record"

        android:textColor="@color/white"
        android:textSize="46px"
        app:layout_constraintBottom_toTopOf="@+id/chronic2"
        app:layout_constraintEnd_toEndOf="@+id/chronic2"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="@+id/chronic2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.842" />

    <TextView
        android:id="@+id/chronic2"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="69dp"
        android:lineSpacingExtra="-0.01sp"
        android:text="Chronic Diseases"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/chronic_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        />

    <TextView
        android:id="@+id/past2"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="477dp"

        android:lineSpacingExtra="-0.01sp"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/pastsmoke"
        app:layout_constraintEnd_toEndOf="@+id/pastsmoke"
        app:layout_constraintStart_toStartOf="@+id/pastsmoke"
        app:layout_constraintTop_toTopOf="parent"
         />
    <TextView
        android:id="@+id/pastot_smoke"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:lineSpacingExtra="-0.01sp"

        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton_history"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pastsmoke"
        app:layout_constraintVertical_bias="0.136"
        />
    <TextView
        android:id="@+id/past3"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="437dp"
        android:lineSpacingExtra="-0.01sp"
        android:text="Past Medications"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/past2"
        app:layout_constraintEnd_toEndOf="@+id/past2"
        app:layout_constraintStart_toStartOf="@+id/past2"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/pastsmoke"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="518dp"
        android:lineSpacingExtra="-0.01sp"
        android:text="Smoking Habits"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintEnd_toEndOf="@+id/pastot_smoke"
        app:layout_constraintStart_toStartOf="@+id/pastot_smoke"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/allg"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="234dp"
        android:lineSpacingExtra="-0.01sp"
        android:text="Allergies"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/allg_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/Blood"
        android:layout_width="270dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="366dp"
        android:lineSpacingExtra="-0.01sp"
        android:text="Blood Type"
        android:textColor=" #7B8497"
        android:textSize="16sp"
        android:translationY="-0.25sp"
        app:layout_constraintBottom_toTopOf="@+id/blood_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="32dp"
        android:layout_height="40dp"
        android:layout_marginEnd="291dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/Medical"
        app:layout_constraintEnd_toEndOf="@+id/Medical"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Medical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pick15"
        />
    <ImageButton
        android:id="@+id/conslt"
        android:layout_width="84dp"
        android:layout_height="68dp"
        android:layout_marginStart="172dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="34dp"
        android:background="@color/white"
        android:src="@drawable/doctorr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/pastot_smoke"
        app:layout_constraintVertical_bias="1.0"
        />
    <ImageButton
        android:id="@+id/profile"
        android:layout_width="62dp"
        android:layout_height="51dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="299dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="48dp"
        android:background="@drawable/pict11"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pastot_smoke"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imageButton_history"
        android:layout_width="81dp"
        android:layout_height="55dp"
        android:layout_marginStart="4dp"

        android:layout_marginTop="92dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/conslt"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pastsmoke"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pict10" />

    <TextView
        android:id="@+id/allg_txt"
        android:layout_width="268dp"
        android:layout_height="38dp"
        android:layout_marginTop="314dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/Blood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/chronic_txt"
        android:layout_width="261dp"
        android:layout_height="35dp"
        android:layout_marginTop="178dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/allg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

</androidx.constraintlayout.widget.ConstraintLayout>